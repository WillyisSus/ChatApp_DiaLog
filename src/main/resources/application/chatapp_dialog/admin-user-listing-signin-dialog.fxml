<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<DialogPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" layoutX="40.0" layoutY="21.0" prefHeight="21.0" prefWidth="601.0" text="Sign in of user - 10 latest records">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <TableView layoutX="20.0" layoutY="97.0" prefHeight="264.0" prefWidth="641.0">
              <columns>
                <TableColumn fx:id="recordnumber" prefWidth="75.0" text="Record No." />
                <TableColumn fx:id="recorddate" prefWidth="75.0" text="Date and Time" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </AnchorPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="CLOSE" />
   </buttonTypes>
</DialogPane>
